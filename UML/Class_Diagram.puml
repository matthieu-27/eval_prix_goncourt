@startuml
class Book {
    +isbn: String
    +title: String
    +summary: Text
    +main_character: String
    +release_date: Date
    +pages_number: int
    +author_name: String
    +author_biography: String
    +editor: Editor
    displayBookDetails(isbn): void
}

class Editor {
    +editor_name: String
}

class Selection {
    +id: int
    +selection_number: int
    +selection_date: Date
    addBookToSelection(isbn): void
    displaySelection(id): Book[]
    displaySelections(): Selection[]
}

class Personality {
    +id: int
    +name: String
    +is_president: Boolean
}

class Selection_Books {
    +number_of_votes: int
    setNumberOfVotes(isbn, number): void
}

class Editor_Books {
    +editor_price: Float
}

'*************Relation des classes*************


Selection "*" -up- "1.." Book : Selection_Books
(Selection, Book) .. Selection_Books

Book "1..*" -left- "1..*" Editor : Editor_Books
(Editor, Book) .. Editor_Books
Personality "1.." -right- "1.." Selection : Jury


@enduml